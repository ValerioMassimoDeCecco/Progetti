{
  "manifest_version": 3,
  "name": "Password Manager",
  "version": "1.0",
  "description": "Gestisci le tue credenziali",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
